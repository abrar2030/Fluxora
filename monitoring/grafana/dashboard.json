{
    "title": "Model Performance",
    "panels": [
      {
        "type": "graph",
        "title": "Prediction Accuracy",
        "targets": [
          {
            "expr": "model_mae",
            "legendFormat": "MAE"
          }
        ]
      },
      {
        "type": "stat",
        "title": "API Requests",
        "targets": [
          {
            "expr": "rate(api_requests_total[5m])",
            "legendFormat": "Requests/s"
          }
        ]
      }
    ]
  }